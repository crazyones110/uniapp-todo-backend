{
  "apps": [
    {
      "name": "mp-todo",
      "script": "build/index.js",
      "log_date_format": "YYYY-MM-DD HH:mm:ss",
      "error_file": "/var/log/node-app/node-app.stderr.log",
      "out_file": "log/node-app.stdout.log",
      "instances": 4,
      "watch": true,
      "ignore_watch": [
        "node_modules",
        "log"
      ]
    }
  ]
}